{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.words": [
    "accesskey",
    "activedescendant",
    "autocorrect",
    "autoplace",
    "Booleanish",
    "clsx",
    "codegen",
    "colcount",
    "colindex",
    "colspan",
    "composables",
    "contenteditable",
    "describedby",
    "dropeffect",
    "enumify",
    "errormessage",
    "favicons",
    "flowto",
    "haspopup",
    "HTMLSVG",
    "hygen",
    "incstr",
    "inlist",
    "inputmode",
    "itemid",
    "itemprop",
    "itemref",
    "itemscope",
    "itemtype",
    "keyshortcuts",
    "labelledby",
    "mpath",
    "multiselectable",
    "nocheck",
    "noindex",
    "Numberish",
    "Omitable",
    "pinia",
    "pngs",
    "posinset",
    "proxify",
    "radiogroup",
    "referrerpolicy",
    "roledescription",
    "rowcount",
    "rowindex",
    "rowspan",
    "scripthost",
    "setsize",
    "tabindex",
    "Transformative",
    "Typle",
    "unimported",
    "valuenow",
    "valuetext",
    "vnode",
    "vuex"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.exclude": {
    "node_modules/": false
  },
  "eslint.format.enable": true,
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.nodePath": "./node_modules",
  "eslint.options": {
    "extensions": [".html", ".js", ".ts", ".jsx", ".tsx"]
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "html",
    "javascriptreact",
    "typescriptreact"
  ]
}
